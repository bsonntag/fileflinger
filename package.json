{
  "name": "fileflinger",
  "version": "0.0.0",
  "description": "Testing out simple-peer for P2P file sharing.",
  "license": "MIT",
  "author": "Benjamim Sonntag <benjamimsonntag@gmail.com>",
  "scripts": {
    "bundle": "parcel build --public-url /fileflinger src/index.html",
    "deploy": "gh-pages --dist dist --message \"Update website\"",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "start": "parcel --out-dir build src/index.html"
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "bytes": "^3.0.0",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "simple-peer": "^9.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-config-seegno": "^10.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "lint-staged": "^7.3.0",
    "parcel-bundler": "^1.9.7",
    "pre-commit": "^1.2.2"
  },
  "lint-staged": {
    "ignore": [
      "build",
      "dist"
    ],
    "linters": {
      "**/*.js": "eslint"
    }
  }
}
